<% provide(:title, 'All users') %>
<div class="container">
<h1>All Users</h1>
<table class="table">
<thead>
	<tr>
	<th> Username </th>
	<th> Email</th>
	<th> Created </th>
</thead>
<tbody>
<% @users.each do |u| %>
<tr>
<td><%= u.username %></td>
<td><%= u.email %></td>
<td><%= u.created_at %></td>
</tr>
<% end %>
</tbody>
</table>
<%= will_paginate %>
</div>